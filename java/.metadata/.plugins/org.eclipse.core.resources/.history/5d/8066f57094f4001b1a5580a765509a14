package com.oops;

public class MotorBike {

	// State
	private int speed; // member variable

	// Behaviour
	// method
	// input - int speed
	// output - void
	// name - setSpeed
	void setSpeed(int speed) { // local variable
		this.speed = speed;
		// System.out.println(speed);
		// System.out.println(this.speed);

	}

	int getSpeed() {
		return this.speed;
	}

	void start() {
		System.out.println("Bike Started");
	}

}
