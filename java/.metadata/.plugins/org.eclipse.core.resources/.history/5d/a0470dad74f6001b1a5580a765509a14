package com.primitive.datatypes;

public class MyChar {

	private char ch;

	public MyChar(char ch) {
		this.ch = ch;

	}

	public Boolean isVowel() {
		if (ch == 'a') {
			return true;
		}

		if (ch == 'e') {
			return true;
		}
		if (ch == 'i') {
			return true;
		}
		if (ch == 'o') {
			return true;
		}
		if (ch == 'u') {
			return true;
		}


		return false;
	}

}
//|| ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') 