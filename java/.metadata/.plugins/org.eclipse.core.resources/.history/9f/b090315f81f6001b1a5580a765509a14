package com.ifStatement.example;

public class ifStatementRunner {

	public static void main(String[] args) {

		int i = 24;
		if (i == 25) {
			System.out.println("i=25");
		}
		
		else if (i==23) {
			System.out.println("i = 23");
		} 
		else {
			System.out.println("i != 25 and i!= 24");
		}

	}

}
