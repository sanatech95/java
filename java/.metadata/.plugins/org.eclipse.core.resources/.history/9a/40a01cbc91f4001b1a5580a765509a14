package com.oops;

public class MotorBike {
	
	//State
	private int speed;
	
	//Behaviour
	//method
	//input - int speed
	//output - void
	//name - setSpeed
	void setSpeed(int speed) {  //local variable
		this.speed = speed;
		//System.out.println(speed);
		//System.out.println(this.speed);
		
	}
	
	void start() {
		System.out.println("Bike Started");
	}

}
